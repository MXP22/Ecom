@model IEnumerable<AccsEco.Models.Commande>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UtilisateurSite.Nom)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UtilisateurSite.Nom)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IDcommande }) |
            @Html.ActionLink("Details", "Details", new { id=item.IDcommande }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.IDcommande })
        </td>
    </tr>
}

</table>
